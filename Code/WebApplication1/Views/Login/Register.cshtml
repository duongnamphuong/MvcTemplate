@{
    ViewBag.Title = Resources.Resource.Register;
    IDictionary<int, string> dict = (IDictionary<int, string>)ViewBag.HashTypeDict;
}
<h1>@ViewBag.Title</h1>
<form action="@Url.Action("Register", "Login")" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">@Resources.Resource.Email_Address</label>
        <input type="text" class="form-control" id="exampleInputEmail1" name="username" />
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">@Resources.Resource.Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1" name="password">
    </div>
    <div class="form-group">
        <label for="exampleInputHashType1">@Resources.Resource.Password_hash_type</label>
        <select class="form-control" id="exampleInputHashType1" name="hashtype">
            <option value="-1" selected="selected">@Resources.Resource.Select_hash_type</option>
            @for (int i = 0; i < dict.Count(); i++)
            {
                <option value="@dict.Keys.ToList()[i]">@dict[dict.Keys.ToList()[i]]</option>
            }
        </select>
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary">@Resources.Resource.Register</button>
    </div>
</form>
<div>
    @Html.ActionLink(Resources.Resource.Return_to_login_page, "Index", "Login")
</div>